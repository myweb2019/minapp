/**index.wxss**/
.index-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index-swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_content {
  overflow: hidden;
}
.index_floor .floor_content navigator {
  float: left;
  width: 33.333%;
  /*后四个超链接*/
}
.index_floor .floor_content navigator:nth-last-child(-n+4) {
  /*
        原图的宽高232*368
        232/386 = 33.33vw/height
        由于后面的图片的高度是第一张的高度的一半所以要除以2
        */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_content navigator:nth-of-type(2),
.index_floor .floor_content navigator:nth-of-type(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_content navigator image {
  width: 100%;
  height: 100%;
}
